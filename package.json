{
  "name": "eolian",
  "version": "1.0.0",
  "description": "A bot for music enthusiasts",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "start": "node -r dotenv/config ./dist/bundle.js -gi",
    "start-debug": "node -r dotenv/config --inspect-brk ./dist/bundle.js -gi",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write ./src && eslint --fix src/**/*.ts"
  },
  "keywords": [
    "music",
    "bot",
    "discord",
    "spotify",
    "soundcloud"
  ],
  "engines": {
    "node": ">=16.13.0"
  },
  "author": "Jack Belford",
  "license": "GPL-3.0+",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.1.0",
    "@discordjs/voice": "^0.11.0",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.37.10",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2",
    "erlpack": "^0.1.4",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.1.0",
    "fuzzball": "^2.1.2",
    "googleapis": "^95.0.0",
    "html-entities": "^2.3.3",
    "iso8601-duration": "^2.1.1",
    "mongodb": "^4.9.1",
    "node-cache": "^5.1.2",
    "node-gyp": "^9.1.0",
    "play-dl": "^1.9.5",
    "prism-media": "^1.3.4",
    "sodium-native": "^3.4.1",
    "undici": "^5.10.0",
    "utf-8-validate": "^5.0.9",
    "winston": "^3.8.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.18",
    "@types/node-cache": "^4.2.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@3.1.0"
}
