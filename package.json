{
  "name": "eolian",
  "version": "1.0.0",
  "description": "A bot for music enthusiasts",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "start": "node -r dotenv/config ./dist/bundle.js -gi",
    "start-debug": "node -r dotenv/config --inspect-brk ./dist/bundle.js -gi",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write ./src && eslint --fix src/**/*.ts"
  },
  "keywords": [
    "music",
    "bot",
    "discord",
    "spotify",
    "soundcloud"
  ],
  "engines": {
    "node": ">=16.13.0"
  },
  "author": "Jack Belford",
  "license": "GPL-3.0+",
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/opus": "0.5.3",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.27.1",
    "discord.js": "^13.5.1",
    "dotenv": "^16.0.0",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^4.4.1",
    "fuzzball": "^2.1.2",
    "googleapis": "^95.0.0",
    "html-entities": "^2.3.2",
    "iso8601-duration": "^1.3.0",
    "mongodb": "^4.3.1",
    "node-cache": "^5.1.2",
    "node-gyp": "^8.4.1",
    "play-dl": "^1.8.9",
    "prism-media": "^1.3.2",
    "sodium": "^3.0.2",
    "undici": "^4.14.1",
    "utf-8-validate": "^5.0.8",
    "winston": "^3.6.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.18",
    "@types/node-cache": "^4.2.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@3.1.0"
}
