    "fuzzball": "^1.4.0",
    "googleapis": "^73.0.0",
    "libsodium-wrappers": "^0.7.9",
    "module-alias": "^2.1.0",
    "mongodb": "^3.6.6",
    "node-cache": "^5.1.2",
    "node-cleanup": "^2.1.2",
    "prism-media": "^1.2.9",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
