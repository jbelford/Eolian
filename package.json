{
  "name": "eolian",
  "version": "1.0.0",
  "description": "A bot for music enthusiasts",
  "private": true,
  "scripts": {
    "build-web": "webpack --config webpack.web.js",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "start": "node -r dotenv/config ./dist/bundle.js -gi",
    "start-debug": "node -r dotenv/config --inspect-brk ./dist/bundle.js -gi",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write ./src"
  },
  "keywords": [
    "music",
    "bot",
    "discord",
    "spotify",
    "soundcloud"
  ],
  "engines": {
    "node": ">=20.17.0"
  },
  "author": "Jack Belford",
  "license": "GPL-3.0+",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.4.0",
    "@discordjs/voice": "^0.18.0",
    "@fluentui/react-components": "^9.43.3",
    "@fluentui/react-migration-v0-v9": "^9.1.3",
    "bgutils-js": "^2.2.0",
    "bufferutil": "^4.0.8",
    "discord-api-types": "^0.37.111",
    "discord.js": "^14.16.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ffmpeg-static": "^5.2.0",
    "fuzzball": "^2.1.2",
    "googleapis": "^109.0.1",
    "html-entities": "^2.3.3",
    "https-proxy-agent": "^7.0.5",
    "iso8601-duration": "^2.1.1",
    "jsdom": "^25.0.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.41.0",
    "mongodb": "^4.11.0",
    "node-cache": "^5.1.2",
    "node-fetch": "2",
    "node-gyp": "^10.2.0",
    "openai": "^4.77.0",
    "prism-media": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sodium-native": "^4.3.1",
    "undici": "^6.19.8",
    "utf-8-validate": "^6.0.4",
    "winston": "^3.8.2",
    "youtubei.js": "^12.2.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/node-cache": "^4.2.5",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/winston": "^2.4.4",
    "copy-webpack-plugin": "^11.0.0",
    "prettier": "^3.4.2",
    "ts-loader": "^9.3.1",
    "typescript": "5.4.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@4.4.1"
}
