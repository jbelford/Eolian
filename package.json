{
  "name": "eolian",
  "version": "0.0.1",
  "description": "A bot for music enthusiasts",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start-dev": "ts-node -r dotenv/config --files src/app.ts -gi",
    "start": "npm install @discordjs/opus && node -r dotenv/config ./dist/bundle.js -gi",
    "check": "gts check"
  },
  "keywords": [
