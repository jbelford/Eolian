{
  "name": "eolian",
  "version": "0.0.1",
  "description": "A bot for music enthusiasts",
  "private": true,
  "scripts": {
    "build": "webpack",
    "test": "node -r dotenv/config node_modules/jest/bin/jest.js --testPathPattern=/test/.+\\.test\\.ts",
    "unit-test-only": "node -r dotenv/config node_modules/jest/bin/jest.js --testPathPattern=/unit/.+\\.test\\.ts",
    "start-dev": "ts-node -r dotenv/config --files src/app.ts",
    "start": "node -r dotenv/config ./dist/bundle.js"
  },
  "keywords": [
    "music",
    "bot",
    "discord",
    "spotify",
    "soundcloud"
  ],
  "author": "Jack Belford",
  "license": "GPL-3.0+",
  "dependencies": {
    "@google-cloud/firestore": "^2.2.3",
    "discord.js": "^11.4.2",
    "dotenv": "^8.0.0",
    "fuzzball": "^1.1.6",
    "googleapis": "^40.0.0",
    "module-alias": "^2.1.0",
    "mongodb": "^3.1.8",
    "node-cache": "^4.2.0",
    "node-cleanup": "^2.1.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.3",
    "spotify-web-api-node": "^4.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.1.12",
    "@types/node": "^10.11.4",
    "@types/node-cache": "^4.1.3",
    "@types/node-cleanup": "^2.1.0",
    "@types/request-promise-native": "^1.0.15",
    "@types/winston": "^2.4.4",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  }
}
